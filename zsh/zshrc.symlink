# Path to your oh-my-zsh configuration.
ZSH=$HOME/.oh-my-zsh
ZSH_CUSTOM=$HOME/.oh-my-zsh-custom

ZSH_THEME="my"

plugins=(gradle git zsh-syntax-highlighting svn composer forklift symfony2 sublime magici wakeonlan)

source $ZSH/oh-my-zsh.sh

# load other scripts from dotfiles
DOTFILES=~/dotfiles
setopt extended_glob
for config_file ($DOTFILES/^(load_excluded)/**/*.zsh) source $config_file
